{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run All Tests",
			"type": "shell",
			"command": "mvn",
			"args": [
				"test"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			}
		},
		{
			"label": "Run InvocationCountConcept Test",
			"type": "shell",
			"command": "mvn",
			"args": [
				"test",
				"-Dtest=InvocationCountConcept"
			],
			"group": {
				"kind": "test",
				"isDefault": false
			}
		},
		{
			"label": "Run TestNGFeatures Test",
			"type": "shell",
			"command": "mvn",
			"args": [
				"test",
				"-Dtest=TestNGFeatures"
			],
			"group": {
				"kind": "test",
				"isDefault": false
			}
		}
	]
}
